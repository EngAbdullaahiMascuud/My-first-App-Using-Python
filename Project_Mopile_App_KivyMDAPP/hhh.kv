MDScreen:
    name:"Dash"
    md_bg_color:"cyan"

    MDBoxLayout:
        MDBoxLayout:
            orientation:"vertical"
            MDBoxLayout:
                AnchorLayout:
                    anchor_y:"top"
                    anchor_x:"left"
                    MDIconButton:
                        icon:"arrow-left"
                        on_press:
